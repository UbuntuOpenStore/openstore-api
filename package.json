{
  "name": "openappstore-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=./dist node -r dotenv/config dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "lint": "eslint ./src/**/*.ts ./tests/**/*.ts --fix",
    "lint:json": "jsonlint -q ./src/api/json/category_icons.json && jsonlint -q ./src/api/json/default_frameworks.json && jsonlint -q ./src/api/json/discover_apps.json",
    "test": "NODE_ENV=testing mocha -r tsconfig-paths/register -r ts-node/register --reporter dot tests/**/*.ts",
    "coverage": "NODE_ENV=testing nyc mocha -r tsconfig-paths/register -r ts-node/register --reporter dot tests/**/*.ts",
    "test:ci": "NODE_ENV=ci nyc mocha -r tsconfig-paths/register -r ts-node/register --reporter dot tests/**/*.ts",
    "test:snapshot": "SNAPSHOT_TEST=true NODE_ENV=testing mocha -r tsconfig-paths/register -r ts-node/register --reporter dot tests/snapshots.test.ts",
    "update-pot-file": "npm run ts-command update-pot-file",
    "command": "bash -c 'NODE_PATH=./dist node -r dotenv/config ./dist/commands/\"$0\".js'",
    "ts-command": "bash -c 'NODE_PATH=./src ts-node ./src/commands/\"$0\".ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/theopenstore/openstore-api.git"
  },
  "author": "Brian Douglass",
  "license": "GPL-3.0",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register -r dotenv/config ./src/index.ts",
    "ext": "ts, js, json"
  },
  "dependencies": {
    "@sentry/node": "^6.15.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "click-parser": "^4.2.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^1.4.0",
    "dotenv": "^10.0.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "gettext-parser": "^4.2.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mime": "^3.0.0",
    "mongoose": "^6.0.13",
    "multer": "^1.4.3",
    "node-gettext": "^3.0.0",
    "p-limit": "^3.1.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-gitlab2": "^5.0.0",
    "passport-localapikey": "^0.0.3",
    "passport-ubuntu": "^0.2.0",
    "rss": "^1.2.2",
    "sanitize-html": "^2.5.3",
    "shuffle-array": "^1.0.1",
    "sleepjs": "^4.0.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.22",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/elasticsearch": "^5.0.40",
    "@types/express": "^4.17.13",
    "@types/gettext-parser": "^4.0.1",
    "@types/lodash": "^4.14.177",
    "@types/method-override": "^0.0.32",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^9.1.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.8",
    "@types/node-gettext": "^3.0.2",
    "@types/passport": "^1.0.7",
    "@types/request": "^2.48.7",
    "@types/rss": "^0.0.29",
    "@types/sanitize-html": "^2.5.0",
    "@types/shuffle-array": "^1.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-bhdouglass": "0.0.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.25.3",
    "jsonlint": "^1.6.3",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2"
  }
}
